rm main4.elf
rm main4.o
rm main4.hex
avr-gcc -mmcu=atmega328p -std=gnu99 -Wall -Os -o main4.elf main4.c -w
avr-objcopy -j .text -j .data -O ihex main4.elf main4.hex
avr-size --mcu=atmega328p --format=avr main4.elf
# fuse = 62 for 1MHz clock = internal 8Meg / division 8
sudo avrdude -c usbasp -p m328p -U lfuse:w:0x62:m  -U flash:w:"main4.hex":a

